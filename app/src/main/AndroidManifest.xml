<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cbs.sscbs">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.INTERACT_ACROSS_USERS_FULL"
        android:protectionLevel="signature" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TeachersTimeTable"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".Model" />
        <activity android:name=".Sample"
            android:label="Info"/>
        <activity android:name=".LifeAtCbs" />
        <activity android:name=".Facilities" />
        <activity
            android:name=".About_Activity"
            android:label="About Us"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Developers"
            android:label="Developers"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Gallery_Activity"
            android:label="GALLERY"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".auth.AuthUiActivity" />
        <activity android:name=".auth.SignedInActivity" />
        <activity
            android:name=".FullScreenImage"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".web" />
        <activity android:name=".Contact_us"
            android:parentActivityName=".LifeAtCbs"
            android:label="Contact-Us"></activity>
        <activity android:name=".Events"></activity>
        <activity android:name=".CreateEvent"></activity>

    </application>

</manifest>