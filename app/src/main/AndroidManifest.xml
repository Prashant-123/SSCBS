        <activity
            android:name=".Others.Gallery_Activity"
            android:label="GALLERY"
            android:parentActivityName=".Others.MainActivity" />
        <activity android:name=".auth.AuthUiActivity" />
        <activity android:name=".auth.SignedInActivity" />
        <activity
            android:name=".Others.FullScreenImage"
